{
  "python.envFile": "${workspaceFolder}/.vscode/.env",
  "editor.formatOnSave": true,
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "clang-format.executable": "${workspaceRoot}/.venv/Scripts/clang-format.exe",
  "cSpell.words": [
    "_ENABLE_ASAN",
    "AIAS",
    "BINDIR",
    "buildkit",
    "CLANGTIDY",
    "conanbuildinfo",
    "cplusplus",
    "cxxflags",
    "DUNICODE",
    "endforeach",
    "endfunction",
    "formating",
    "fprofile",
    "fsanitize",
    "ftest",
    "gmock",
    "imgui",
    "INCLUDEDIR",
    "INLINES_",
    "Interprocedural",
    "IWYU",
    "libcommon",
    "LPWSTR",
    "Miscelanious",
    "NOMINMAX",
    "RTBS",
    "STREQUAL",
    "suppr",
    "Vcpkg",
    "Wcast",
    "Wconversion",
    "Wdouble",
    "Wduplicated",
    "Werror",
    "Wextra",
    "Wformat",
    "Wlogical",
    "Wmisleading",
    "Wnon",
    "Wnull",
    "Woverloaded",
    "Wpedantic",
    "Wshadow",
    "Wsign",
    "Wunused",
    "Wuseless"
  ]
}
